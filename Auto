-- Di chuyển menu
local function updatePosition(input)
    local delta = input.Position - dragStart
    mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

mainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = mainFrame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

mainFrame.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        updatePosition(input)
    end
end)

-- Tạo Hitbox bao quanh người chơi
local function createHitbox(radius)
    if hitbox then hitbox:Destroy() end
    hitbox = Instance.new(lVMTNlpZHKTGgIUabFGhQn('vSQojkdbejoPTVwShCpMqeAwlKyxqzJTzwsqNAgKrLStYQUUOimgaiEUGFydA=='))
    hitbox.Shape = Enum.PartType.Ball
    hitbox.Size = Vector3.new(radius * 2, radius * 2, radius * 2)
    hitbox.Anchored = true
    hitbox.CanCollide = false
    hitbox.Material = Enum.Material.Neon
    hitbox.Color = Color3.fromRGB(255, 255, 255) -- Màu trắng ban đầu
    hitbox.Transparency = 0.5
    hitbox.Parent = workspace
end

-- Cập nhật vị trí hitbox và chuyển màu
local function updateHitbox()
    if hitbox and player.Character and player.Character:FindFirstChild(lVMTNlpZHKTGgIUabFGhQn('ZIWtKKLTdhwFeXEdIkcELeXHSCkuBtAueoTKTUgaqREOIIxiwfeihmXSHVtYW5vaWRSb290UGFydA==')) then
        hitbox.Position = player.Character.HumanoidRootPart.Position
    end
end

-- Chuyển màu hitbox và thay đổi kích thước tùy theo tốc độ bóng
local function colorCycle(ball)
    if not ball then return end
    local ballPos = ball.Position
    local playerPos = player.Character.HumanoidRootPart.Position
    local distance = (ballPos - playerPos).Magnitude

    -- Nếu bóng gần người chơi, chuyển màu hitbox thành đỏ
    if distance <= 5 then
        hitbox.Color = Color3.fromRGB(255, 0, 0) -- Màu đỏ khi bóng gần
    else
        hitbox.Color = Color3.fromRGB(255, 255, 255) -- Màu trắng khi bóng xa
    end

    -- Thay đổi kích thước hitbox tùy theo tốc độ bóng (magnitude)
    local newRadius = math.clamp(distance / 2, 5, 15) -- Giới hạn kích thước hitbox từ 5 đến 15
    hitbox.Size = Vector3.new(newRadius * 2, newRadius * 2, newRadius * 2)
end

-- Auto Parry 4.0 Pro
local autoParryActive = false
local function autoParry()
    createHitbox(5) -- Tạo hitbox với bán kính 5m
    spawn(function()
        while autoParryActive do
            if player.Character and player.Character:FindFirstChild(lVMTNlpZHKTGgIUabFGhQn('LZMhjDyFaqvmYefSVzGKpjLliaHvnXVzURQkwfJTIzsqwVJhQiGznlTSHVtYW5vaWRSb290UGFydA==')) then
                local ball = workspace:FindFirstChild(lVMTNlpZHKTGgIUabFGhQn('rlMRKgcmMuWSisoRqQCLAnyxTOtBtGvXafeXHPYyPHCTAhtZRdFIngrQmFsbA==')) -- Cập nhật nếu tên khác
                if ball then
                    local playerPos = player.Character.HumanoidRootPart.Position
                    local ballPos = ball.Position
                    local distance = (ballPos - playerPos).Magnitude

                    -- Kiểm tra nếu bóng vào trong hitbox
                    if distance <= 5 then
                        print(lVMTNlpZHKTGgIUabFGhQn('PONcVsvXDjpSqeFcpBwBUgGpRuQmhjITXGBRmHTXTpNjPsxtKmmDAfdQXV0byBQYXJyeTogQmxvY2tpbmcgd2l0aCBGIQ=='))
                        -- Gửi phím F để thực hiện hành động Block
                        local success, errorMessage = pcall(function()
                            game:GetService(lVMTNlpZHKTGgIUabFGhQn('NFYJsCyAHeJlvfAmylUDPATqjZnvPUMhzsKbCmjsbxaflBxbBCPNGPxVmlydHVhbElucHV0TWFuYWdlcg==')):SendKeyEvent(true, Enum.KeyCode.F, false, game)
                            wait(0.1)
                            game:GetService(lVMTNlpZHKTGgIUabFGhQn('fUCkUDOBJRHpDhKJLfTJdjQiKdJrujEARZMfKHTlMhyLudijAXwzlrfVmlydHVhbElucHV0TWFuYWdlcg==')):SendKeyEvent(false, Enum.KeyCode.F, false, game)
                        end)
                        if not success then
                            warn(lVMTNlpZHKTGgIUabFGhQn('nZbbieUkujjAHpnVPdakFfpSlYCnObDthigZsXvvmMQKCORcIuhWIysRmFpbGVkIHRvIHNlbmQga2V5IGV2ZW50OiA=') .. errorMessage)
                        end
                    end
                end
                colorCycle(ball)  -- Cập nhật màu sắc hitbox dựa trên bóng
                updateHitbox()
            end
            wait(0.05)
        end
        if hitbox then hitbox:Destroy() end
    end)
end

autoParrySwitch.MouseButton1Click:Connect(function()
    autoParryActive = not autoParryActive
    autoParrySwitch.Text = lVMTNlpZHKTGgIUabFGhQn('gaBEvTGaLlbTpDAUwELlPoPQEYiRMbVNRXpmYlDuaBtKzhSXgMevAERQXV0byBQYXJyeTog') .. (autoParryActive and lVMTNlpZHKTGgIUabFGhQn('YwEKlsgGqmmbKgMzupgDfaawztMnXXGSunmZvCewOGZrrtuOcDdhvxOT04=') or lVMTNlpZHKTGgIUabFGhQn('xwldPhwSeoGvbYkMAmuOBuQnduAjtkPbrBXGFkOVDrfOQXnQuHfKeazT0ZG'))
    if autoParryActive then
        spawn(autoParry)
    end
end)

